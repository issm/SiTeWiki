<!doctype html>
<htmL>
<head>
  <meta http-equiv="content-type" content="text/html; charset=utf-8" />
  <title><: $title || 'SiTeWiki' :></title>
  <meta http-equiv="Content-Style-Type" content="text/css" />
  <meta http-equiv="Content-Script-Type" content="text/javascript" />
  <meta name="viewport" content="width=device-width, minimum-scale=1.0, maximum-scale=1.0"]]>
  <meta name="format-detection" content="telephone=no" />
  <link rel="stylesheet" href="<: uri_for('/static/bootstrap/css/bootstrap.min.css') :>">
  <link href="<: static_file('/static/css/main.css') :>" rel="stylesheet" type="text/css" media="screen" />
  <script type="text/javascript" src="<: uri_for('/static/js/jquery-1.8.1.min.js') :>"></script>
  <script type="text/javascript" src="<: uri_for('/static/bootstrap/js/bootstrap.min.js') :>"></script>
  <script src="<: static_file('/static/js/main.js') :>"></script>
  <!--[if lt IE 9]>
    <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->

: block head_additional -> {}
</head>
<body>
  <header>
    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <h1 class="brand"><a href="<: uri_for('/') :>">SiTeWiki</a></h1>
          <ul class="nav">
            <li class="active"><a href="<: uri_for('/') :>">Home</a></li>
          </ul>
        </div>
      </div>
    </div>
  </header>

  <div class="container" id="content">
: block content -> {}
  </div>

  <footer>
    Powered by <a href="https://github.com/issm/SiTeWiki">SiTeWiki</a>.
  </footer>
</htmL>
