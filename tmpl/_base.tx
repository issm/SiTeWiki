<!doctype html>
<htmL>
<head>
  <meta http-equiv="content-type" content="text/html; charset=utf-8" />
  <title><: $title || 'SiTeWiki' :></title>
  <meta http-equiv="Content-Style-Type" content="text/css" />
  <meta http-equiv="Content-Script-Type" content="text/javascript" />
  <meta name="viewport" content="width=device-width, minimum-scale=1.0, maximum-scale=1.0"]]>
  <meta name="format-detection" content="telephone=no" />
  <script src="<: uri_for('/static/js/jquery-1.7.1.min.js') :>"></script>
  <link href="<: uri_for('/static/bootstrap/bootstrap.min.css') :>" rel="stylesheet" type="text/css" />
  <script src="<: uri_for('/static/bootstrap/bootstrap-dropdown.js') :>"></script>

  <link href="<: static_file('/static/css/main.css') :>" rel="stylesheet" type="text/css" media="screen" />
  <script src="<: static_file('/static/js/main.js') :>"></script>
  <!--[if lt IE 9]>
    <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->

: block head_additional -> {}
</head>
<body>
  <header>
    <div class="topbar-wrapper" style="z-index: 5;">
      <div class="topbar" data-dropdown="dropdown">
        <div class="topbar-inner">
          <div class="container">
            <h3><a href="<: uri_for('/') :>">SiTeWiki</a></h3>
            <ul class="nav">
              <li class="active"><a href="<: uri_for('/') :>">Home</a></li>
            </ul>
          </div>
        </div><!-- /topbar-inner -->
      </div><!-- /topbar -->
    </div>
  </header>

  <div class="container" id="content">
: block content -> {}
  </div>

  <footer>
    Powered by <a href="https://github.com/issm/SiTeWiki">SiTeWiki</a>.
  </footer>
</htmL>
